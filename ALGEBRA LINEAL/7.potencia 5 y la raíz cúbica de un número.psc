Proceso potencia_5_y_la_raíz_cúbica_de_un_número
	Escribir "Digite el numero para hallar la potencia 5 y raiz " Sin Saltar ;
	leer numer ;
	//formuñla para potencia X^3
	//formula para raiz X^(1/3)
	potencia<-numer^5 ;
	raizz<- numer^(1/3) ;
	Escribir "Los resultados son : " ;
	Escribir "****************************" ;
	Escribir "    Potencia 5 : " potencia  ;
	Escribir "    Raiz       : " raizz;
	Escribir "****************************" ;
FinProceso
